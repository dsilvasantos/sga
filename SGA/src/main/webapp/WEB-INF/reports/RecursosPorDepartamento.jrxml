	<parameter name="departamento" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT departamento.nome as departamento,celula.nome as celula,
aplicacao.nome as aplicacao,recursos.NOME as recurso,valor,data
FROM coleta,aplicacao,recursos,departamento,celula
where departamento.id=$P{departamento} and celula.ID_DEPARTAMENTO=departamento.id and
aplicacao.ID_CELULA=celula.id and coleta.aplicacao=aplicacao.ID and
recursos.ID=coleta.recurso and data between $P{dataInicial} and $P{dataFinal}
order by aplicacao.nome,recursos.NOME,coleta.data]]>
	</queryString>
